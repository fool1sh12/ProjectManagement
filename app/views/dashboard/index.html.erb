<h1>Dashboard</h1>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Completion (percentage of tasks complete)</th>
      <th>Overdue Tasks (count)</th>
    </tr>
  </thead>
  <tbody>
    <% Project.all.each do |project| %>
      <tr>
        <td><%= link_to project.name, project_path(project) %></td>
        <td><%=  %></td>
        <td><%=  %></td>
      </tr>
    <% end %>
  </tbody>
</table>

